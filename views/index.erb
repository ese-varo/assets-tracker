<h1>Assets Listing</h1>
<ul>
  <% @assets.each do |a| %>
    <% p a %>
    <li>
      <p>
        Asset Type: <%= a[:type] %> </br>
        Available: <%= a[:available] %> </br>
        <% unless a[:available] %>
          User: <%= a[:username] %>
        <% end %>
      </p>
    </li>
  <% end %>
</ul>
